    trigger: 'click',
    placement: 'bottom',
    container: '#watchlist_share .icon',
    html: true,
    content: $('#watchlist_share_popover'),
  }).on('show.bs.popover', function () {
    $('.cb-share-popover').removeClass('hidden');
