  <script type="text/javascript">
  $(function(){
    // fetch root comments
    CB.comments($('#root-comments'), function (response) {
      // enable join discussion and hide comments frame if no starting comments
      if (response.comments.comments.length === 0) {
        $('.comments-frame').hide();
      }
      updateLastId(response);
      cbEffects.bindEffects();
    });
  });
  </script>
