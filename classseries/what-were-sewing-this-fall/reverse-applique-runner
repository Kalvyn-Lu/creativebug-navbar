            <div data-count="[!=comment.replies_count !]" id="expand-replies-[!=comment.id !]" class="[!=comment.expand_replies_class !]">
              <a onclick="expandReplies([!=comment.id !]);" href="javascript:;">Expand Replies (<span class="reply-count">[!=comment.replies_count !]</span>)</a>
            </div>
          </div>
        </div>
        <div class="col-xs-12 col-sm-3">
          <div class="comment-nav">
            <div class="comment-date">[!=comment.display_date !]</div>
            <div class="[!=comment.reply_class !] js-register"
                data-register-ga-label="comments">Reply</div>
            <div class="comment-[!=comment.id !]-like [!= comment.like_class !] js-register"
                data-register-ga-label="comments">
              <span class="state-text"></span>
              <span class="bullet"></span>
              <span class="heart"></span>
              <span class="likes-total">
                [!= comment.likers.length !]
              </span>
            </div>
            <div class="[!= comment.delete_class !] js-register"
                data-register-ga-label="comments">
              Delete
            </div>
            <div class="[!= comment.report_class !] js-register"
                data-register-ga-label="comments">
              Report
            </div>
          </div>
        </div>
      </div>
    </div>
    <div id="replies-[!=comment.id !]" class="replies"></div>
    <div id="reply-[!=comment.id !]" class="comment-reply">
      <div class="row">
        <div class="col-xs-12" style="padding-top: 10px; padding-right: 30px;">
