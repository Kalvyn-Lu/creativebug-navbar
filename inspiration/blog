<script>
$(function () {
  (window.onresize = function(event) {
    var h = $(window).height();
    var w = $(window).width();
    var p = 0.5;
    if (w<400) p = 0.4;
    $('#calendar td:nth-child(2)').css({'max-width':(w*p>450?450:w*p)});
    $('.dlg-box div').css({'max-height':h-90});
  })();
  setTimeout(function(){
    $('#calendar .cal').removeClass('addthisevent-drop');
  },100);
});

</script>

