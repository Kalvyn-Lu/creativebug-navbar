  <script type="text/javascript">

  $(function(){

    var tutorialsWrapper = $(".tutorials-wrapper");
    var loading = "<img src='/ui/images/loader_30.gif' style='float:right;margin-top:-40px'/>";

    $(document).on('click', '.pagenav a', function (e) {
      e.preventDefault();
      var self, url;

      self = $(this);
      url = self.attr('href');

      $.ajax({
        url: url,
        type: 'post',
        beforeSend: function() {
          tutorialsWrapper.html(loading);
        },
        success: function(response) {

          $(".request-ajax.current").removeClass('current');
          self.addClass('current');

          tutorialsWrapper.fadeOut(500,function(){
            $(this).html(response).fadeIn(500);
          })

        // Push State support
        // CBUG-333
        if( typeof history.pushState === 'function' ) {

          var matched = url.match(/\?.+/);

          if( matched ) {

            matched = matched[0];

          } else {

            matched = location.pathname;

          }

          history.pushState({}, '', matched);
        }

        }
      });


    });

  });

  </script>
  <div class="push"></div>
