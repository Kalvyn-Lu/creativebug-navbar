<script type="text/javascript">
var current = null;

function setPlayer(t) {
  current = $(t).data('order');
  var next = current + 1;
  if (next == $('.playbut').length) {
    next = 0;
  }
  var n = $(".playbut[data-order='"+next+"']");
  var u = 'https://www.creativebug.com/cbtv?video='+$(t).data('video');
  var fburl = 'https://www.facebook.com/sharer/sharer.php?u='+u;

  $('.share a').attr('href', fburl);

  $('#player iframe').attr('src','/ajax/modal/video/'+$(t).data('video'));
  $('#current-title').html( $(t).data('title') );
  $('#current-desc').html( $(t).data('description') );
  $('#next-title').html( $(n).data('title') );
  $('#next-desc').html( $(n).data('description') );

  ga('send', 'event', {
    eventCategory: 'cbtv_videos',
    eventAction: 'view',
    eventLabel: $(t).data('video') + '-' + $(t).data('title'),
    nonInteraction: true
  });

}

$(function(){

  // Play CBTV Intro
  $('.playintro').on('click', function() {
//    $('#player iframe').attr('src','https://www.creativebug.com/ajax/modal/video/instructor/cbtv');
    $("#introplayer").data('order', -1);
    $("#introplayer").data('video', 'instructor/cbtv');
    $("#introplayer").data('title', 'Introducing cbtv');
    $("#introplayer").data('description', "Welcome to Creativebug Television (or CBTV for short), where you can get a "
      + "deeper look into the processes,studios, and daily lives of your favorite artists. Check out our exclusive "
      + "documentaries, HD versions of our Live Facebook videos, mesmerizing timelapses, and much more. All of this "
      + "content is free and shareable. We hope you’ll binge watch and spread the love.");
    setPlayer( $("#introplayer") );
    $('#next-view').show();
    $('#player').modal('show');
  });

  // Play Video Item
  $('.playbut').on('click', function() {
    setPlayer( this );
    $('#next-view').show();
    $('#player').modal('show');
  });

  // Disable Video Player when closing player modal
  $('#player').on('hidden.bs.modal', function (e) {
    $('#player iframe').attr('src','');
  });

  // Play Next Video Item
  $('#next-title').on('click', function() {
    var next = current + 1;
    if (next == $('.playbut').length) {
      next = 0;
    }
    setPlayer( $(".playbut[data-order='"+next+"']") );
  });

  // Play Next Video Item
  $('#up-next').on('click', function() {
    var next = current + 1;
    if (next == $('.playbut').length) {
      next = 0;
    }
    setPlayer( $(".playbut[data-order='"+next+"']") );
  });

  // Display the rest of video items
  $('.view-all').on('click', function() {
    var id = $(this).data('id');


    $('.more-' + id).show();
    $('html, body').animate({
      scrollTop: $('.more-' + id).offset().top - 450
    }, 300);


  });

  // Set initial order
  $('.playbut').each(function(i){
    $(this).attr('data-order', i);
  });

  // Catch JWPlayer onComplete event to play next video
  var eventMethod = window.addEventListener ? "addEventListener" : "attachEvent";
  var eventer = window[eventMethod];
  var messageEvent = eventMethod == "attachEvent" ? "onmessage" : "message";

  eventer(messageEvent, function(e) {
    var key = e.message ? "message" : "data";
    var data = e[key];
    if (data == 'onComplete') {
      current ++;
      if (current == $('.playbut').length) {
        current = 0;
      }
      $('.playbut').each(function(i){
        if ($(this).data('order') == current) {
          // Play Next Video
          setPlayer( this );
        }
      });
    }
  }, false);

  });

</script>

